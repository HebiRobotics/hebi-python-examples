<!-- Igor Body -->
<robot version="1.3.0" description="Igor">
  <rigid-body mass="0">
    <output trans="0 0.1 0.2">
      <include path="igorArm-Left.hrdf"/>
    </output>
    <output trans="0 -0.1 0.2">
      <include path="igorArm-Right.hrdf"/>
